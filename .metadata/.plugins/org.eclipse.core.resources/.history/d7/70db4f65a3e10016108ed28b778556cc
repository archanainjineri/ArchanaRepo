
/*
 * Write code to decode strings. For example, String str = "3[a2[bd]g4[ef]h]", 
 * the output should be "abdbdgefefefefhabdbdgefefefefhabdbdgefefefefh". 
 */
package facebook;
public class StringDisplay {

	public static void main(String[] args) {
		for(int i = 0 ; i< 6 ; i++){
			for(int j=0 ; j<i;j++){
			System.out.print("");
			}
			System.out.print("*");
			System.out.println();
		}

	}
	
}

